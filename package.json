{
  "name": "@adenin/generator-cloud-function",
  "version": "2.8.0",
  "description": "Generate an adenin cloud function repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/adenin-platform/generator-cloud-function"
  },
  "bugs": {
    "url": "https://github.com/adenin-platform/generator-cloud-function/issues"
  },
  "author": "olliejm",
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "adenin",
    "cloud",
    "function",
    "function-as-a-service",
    "faas",
    "azure",
    "aws",
    "lambda",
    "gcp",
    "compute",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.16.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "chalk": "^2.4.2",
    "yeoman-generator": "^2.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged"
  }
}
